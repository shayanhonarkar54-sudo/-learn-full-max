                ]
            },
            korean: {
                name: 'Korean',
                flag: 'üá∞üá∑',
                flashcards: [
                    { word: 'ÏïàÎÖïÌïòÏÑ∏Ïöî', translation: 'Hello', example: 'ÏïàÎÖïÌïòÏÑ∏Ïöî, ÎßåÎÇòÏÑú Î∞òÍ∞ÄÏõåÏöî' },
                    { word: 'Í∞êÏÇ¨Ìï©ÎãàÎã§', translation: 'Thank you', example: 'Ï†ïÎßê Í∞êÏÇ¨Ìï©ÎãàÎã§' },
                    { word: 'ÏïàÎÖïÌûà Í∞ÄÏÑ∏Ïöî', translation: 'Goodbye', example: 'ÏïàÎÖïÌûà Í∞ÄÏÑ∏Ïöî' },
                    { word: 'ÎÑ§', translation: 'Yes', example: 'ÎÑ§, ÏïåÍ≤†ÏäµÎãàÎã§' },
                    { word: 'Î¨º', translation: 'Water', example: 'Î¨º Ï£ºÏÑ∏Ïöî' }
                ],
                quizzes: [
                    { question: 'How do you say "Hello"?', options: ['ÏïàÎÖïÌïòÏÑ∏Ïöî', 'Í∞êÏÇ¨Ìï©ÎãàÎã§', 'ÏïàÎÖïÌûà Í∞ÄÏÑ∏Ïöî', 'Î¨º'], correct: 'ÏïàÎÖïÌïòÏÑ∏Ïöî' }
                ],
                writing: [
                    { prompt: 'I love Korea', answer: 'ÌïúÍµ≠ÏùÑ ÏÇ¨ÎûëÌï¥Ïöî' }
                ]
            },
            chinese: {
                name: 'Chinese',
                flag: 'üá®üá≥',
                flashcards: [
                    { word: '‰Ω†Â•Ω', translation: 'Hello', example: '‰Ω†Â•ΩÔºå‰Ω†Â•ΩÂêóÔºü' },
                    { word: 'Ë∞¢Ë∞¢', translation: 'Thank you', example: 'ÈùûÂ∏∏ÊÑüË∞¢‰Ω†' },
                    { word: 'ÂÜçËßÅ', translation: 'Goodbye', example: 'ÂÜçËßÅÔºåÊòéÂ§©ËßÅ' },
                    { word: 'ÊòØ', translation: 'Yes', example: 'ÊòØÁöÑÔºåÊ≤°Èîô' },
                    { word: 'Ê∞¥', translation: 'Water', example: 'ÊàëË¶ÅÂñùÊ∞¥' }
                ],
                quizzes: [
                    { question: 'How do you say "Thank you"?', options: ['‰Ω†Â•Ω', 'Ë∞¢Ë∞¢', 'ÂÜçËßÅ', 'Ê∞¥'], correct: 'Ë∞¢Ë∞¢' }
                ],
                writing: [
                    { prompt: 'I am Chinese', answer: 'ÊàëÊòØ‰∏≠ÂõΩ‰∫∫' }
                ]
            }
        };

        // State
        let currentLanguage = null;
        let currentMode = 'flashcards';
        let currentCardIndex = 0;
        let currentQuizIndex = 0;
        let currentWritingIndex = 0;
        let quizScore = 0;
        let userStats = {
            wordsLearned: 0,
            xp: 0,
            correctAnswers: 0,
            totalAnswers: 0,
            streak: 12
        };

        // Initialize
        document.addEventListener('DOMContentLoaded', () => {
            createBackgroundAnimation();
            generateStreakCalendar();
            updateStats();
        });

        function createBackgroundAnimation() {
            const container = document.getElementById('bgAnimation');
            for (let i = 0; i < 20; i++) {
                const span = document.createElement('span');
                span.style.left = Math.random() * 100 + '%';
                span.style.animationDelay = Math.random() * 25 + 's';
                span.style.animationDuration = (15 + Math.random() * 10) + 's';
                container.appendChild(span);
            }
        }

        function selectLanguage(lang) {
            currentLanguage = lang;
            
            // Update UI
            document.querySelectorAll('.language-card').forEach(card => {
                card.classList.remove('active');
            });
            event.currentTarget.classList.add('active');
            
            // Show learning section
            setTimeout(() => {
                document.getElementById('learn').classList.add('active');
                document.getElementById('learn').scrollIntoView({ behavior: 'smooth' });
                loadFlashcard();
            }, 300);
        }

        function showSection(section) {
            document.querySelectorAll('.app-section').forEach(s => s.classList.remove('active'));
            if (section === 'learn' && currentLanguage) {
                document.getElementById('learn').classList.add('active');
            } else if (section === 'stats') {
                document.getElementById('stats').classList.add('active');
                updateStats();
            } else if (section === 'home') {
                window.scrollTo({ top: 0, behavior: 'smooth' });
            }
        }

        function switchMode(mode) {
            currentMode = mode;
            
            // Update tabs
            document.querySelectorAll('.mode-tab').forEach(tab => tab.classList.remove('active'));
            event.currentTarget.classList.add('active');
            
            // Show content
            document.querySelectorAll('.mode-content').forEach(content => content.style.display = 'none');
            document.getElementById(mode + 'Mode').style.display = 'block';
            
            // Load content
            if (mode === 'flashcards') loadFlashcard();
            if (mode === 'quiz') loadQuiz();
            if (mode === 'writing') loadWriting();
        }

        // Flashcard Functions
        function loadFlashcard() {
            if (!currentLanguage) return;
            const card = languageData[currentLanguage].flashcards[currentCardIndex];
            document.getElementById('cardWord').textContent = card.word;
            document.getElementById('cardTranslation').textContent = card.translation;
            document.getElementById('cardExample').textContent = '"' + card.example + '"';
            document.getElementById('flashcard').classList.remove('flipped');
        }

        function flipCard() {
            document.getElementById('flashcard').classList.toggle('flipped');
        }

        function nextCard() {
            currentCardIndex = (currentCardIndex + 1) % languageData[currentLanguage].flashcards.length;
            loadFlashcard();
        }

        function easyCard() {
            userStats.xp += 10;
            userStats.wordsLearned++;
            updateStats();
            nextCard();
            showNotification('Great! +10 XP');
        }

        function difficultCard() {
            userStats.xp += 5;
            updateStats();
            nextCard();
            showNotification('Keep practicing! +5 XP');
        }

        // Quiz Functions
        function loadQuiz() {
            if (!currentLanguage) return;
            const quiz = languageData[currentLanguage].quizzes[currentQuizIndex];
            document.getElementById('quizQuestion').textContent = quiz.question;
            document.getElementById('currentQuestion').textContent = currentQuizIndex + 1;
            
            const optionsContainer = document.getElementById('quizOptions');
            optionsContainer.innerHTML = '';
            quiz.options.forEach(option => {
                const btn = document.createElement('button');
                btn.className = 'quiz-option';
                btn.textContent = option;
                btn.onclick = () => checkAnswer(btn, option);
                optionsContainer.appendChild(btn);
            });
        }

        function checkAnswer(btn, answer) {
            const quiz = languageData[currentLanguage].quizzes[currentQuizIndex];
            const isCorrect = answer === quiz.correct;
            
            userStats.totalAnswers++;
            if (isCorrect) {
                btn.classList.add('correct');
                userStats.correctAnswers++;
                userStats.xp += 20;
                quizScore += 20;
                showNotification('Correct! +20 XP');
            } else {
                btn.classList.add('wrong');
                showNotification('Wrong answer. Try again!');
            }
            
            document.getElementById('quizScore').textContent = quizScore;
            updateStats();
            
            setTimeout(() => {
                currentQuizIndex = (currentQuizIndex + 1) % languageData[currentLanguage].quizzes.length;
                loadQuiz();
            }, 1500);
        }

        // Writing Functions
        function loadWriting() {
            if (!currentLanguage) return;
            const writing = languageData[currentLanguage].writing[currentWritingIndex];
            document.getElementById('writingPrompt').textContent = '"' + writing.prompt + '"';
            document.getElementById('writingInput').value = '';
            document.getElementById('writingFeedback').classList.remove('show');
        }

        function checkWriting() {
            const input = document.getElementById('writingInput').value.trim().toLowerCase();
            const writing = languageData[currentLanguage].writing[currentWritingIndex];
            const feedback = document.getElementById('writingFeedback');
            
            if (input === writing.answer.toLowerCase()) {
                feedback.textContent = '‚úÖ Correct! Great job!';
                feedback.className = 'feedback correct show';
                userStats.xp += 15;
                userStats.correctAnswers++;
                showNotification('Excellent! +15 XP');
                setTimeout(() => {
                    currentWritingIndex = (currentWritingIndex + 1) % languageData[currentLanguage].writing.length;
                    loadWriting();
                }, 2000);
            } else {
                feedback.textContent = '‚ùå Not quite. The answer is: ' + writing.answer;
                feedback.className = 'feedback wrong show';
            }
            userStats.totalAnswers++;
            updateStats();
        }

        // Listening Functions
        function playAudio() {
            const utterance = new SpeechSynthesisUtterance('Hello, how are you today?');
            utterance.lang = currentLanguage === 'spanish' ? 'es-ES' : 
                           currentLanguage === 'french' ? 'fr-FR' : 
                           currentLanguage === 'german' ? 'de-DE' : 
                           currentLanguage === 'japanese' ? 'ja-JP' : 
                           currentLanguage === 'korean' ? 'ko-KR' : 
                           currentLanguage === 'chinese' ? 'zh-CN' : 'en-US';
            speechSynthesis.speak(utterance);
        }

        function checkListening() {
            const input = document.getElementById('listeningInput').value.trim().toLowerCase();
            const feedback = document.getElementById('listeningFeedback');
            
            if (input.includes('hello') || input.includes('how are you')) {
                feedback.textContent = '‚úÖ Correct! Well done!';
                feedback.className = 'feedback correct show';
                userStats.xp += 20;
                showNotification('Perfect! +20 XP');
            } else {
                feedback.textContent = '‚ùå Try again! Listen carefully.';
                feedback.className = 'feedback wrong show';
            }
            updateStats();
        }

        // Stats Functions
        function updateStats() {
            document.getElementById('totalWords').textContent = userStats.wordsLearned;
            document.getElementById('totalXP').textContent = userStats.xp;
            document.getElementById('gemCount').textContent = Math.floor(userStats.xp / 10);
            
            const accuracy = userStats.totalAnswers > 0 ? 
                Math.round((userStats.correctAnswers / userStats.totalAnswers) * 100) : 0;
            document.getElementById('accuracy').textContent = accuracy + '%';
            
            const hours = Math.floor(userStats.xp / 60);
            document.getElementById('timeSpent').textContent = hours + 'h';
        }

        function generateStreakCalendar() {
            const calendar = document.getElementById('streakCalendar');
            const days = ['S', 'M', 'T', 'W', 'T', 'F', 'S'];
            
            for (let i = 0; i < 28; i++) {
                const day = document.createElement('div');
                day.className = 'streak-day';
                if (i < 7) day.textContent = days[i];
                
                if (i >= 7) {
                    const dayNum = i - 6;
                    if (dayNum <= 12) day.classList.add('active');
                    if (dayNum === 12) day.classList.add('today');
                }
                calendar.appendChild(day);
            }
        }

        function showNotification(message) {
            const notif = document.createElement('div');
            notif.style.cssText = `
                position: fixed;
                top: 100px;
                right: 20px;
                background: linear-gradient(135deg, var(--primary), var(--secondary));
                color: white;
                padding: 1rem 2rem;
                border-radius: 12px;
                font-weight: 600;
                z-index: 10000;
                animation: slideIn 0.3s ease;
                box-shadow: 0 10px 30px rgba(0,0,0,0.2);
            `;
            notif.textContent = message;
            document.body.appendChild(notif);
            
            setTimeout(() => {
                notif.remove();
            }, 2000);
        }
    </script>
</body>
</html>'''

# Save to file
with open('/mnt/kimi/output/learn_full_max.html', 'w', encoding='utf-8') as f:
    f.write(html_code)

