                    { word: 'Nein', translation: 'No', example: 'Nein, danke' }
                ],
                quizzes: [
                    { question: 'How do you say "Thank you"?', options: ['Danke', 'Hallo', 'Ja', 'Bitte'], correct: 'Danke' }
