                quizzes: [
                    { question: 'How do you say "Thank you"?', options: ['„Åì„Çì„Å´„Å°„ÅØ', '„ÅÇ„Çä„Åå„Å®„ÅÜ', '„Åï„Çà„ÅÜ„Å™„Çâ', '„Åä„ÅØ„Çà„ÅÜ'], correct: '„ÅÇ„Çä„Åå„Å®„ÅÜ' },
                    { question: 'What does "Ê∞¥" mean?', options: ['Fire', 'Water', 'Earth', 'Wind'], correct: 'Water' }
                ],
                writing: [
                    { prompt: 'Hello, nice to meet you', answer: '„Åì„Çì„Å´„Å°„ÅØ„ÄÅ„ÅØ„Åò„ÇÅ„Åæ„Åó„Å¶' },
                    { prompt: 'I like sushi', answer: 'ÂØøÂè∏„ÅåÂ•Ω„Åç„Åß„Åô' }
                ]
            },
            french: {
                name: 'French',
                flag: 'üá´üá∑',
                flashcards: [
                    { word: 'Bonjour', translation: 'Hello', example: 'Bonjour, comment allez-vous?' },
                    { word: 'Merci', translation: 'Thank you', example: 'Merci beaucoup!' },
                    { word: 'Au revoir', translation: 'Goodbye', example: 'Au revoir et √† bient√¥t' },
                    { word: 'Oui', translation: 'Yes', example: 'Oui, je comprends' },
                    { word: 'Non', translation: 'No', example: 'Non, merci' }
                ],
                quizzes: [
                    { question: 'How do you say "Thank you"?', options: ['Merci', 'Bonjour', 'Au revoir', 'Oui'], correct: 'Merci' },
                    { question: 'What does "Oui" mean?', options: ['No', 'Yes', 'Maybe', 'Please'], correct: 'Yes' }
                ],
                writing: [
                    { prompt: 'I am a student', answer: 'Je suis √©tudiant' },
                    { prompt: 'The food is delicious', answer: 'La nourriture est d√©licieuse' }
                ]
            },
            german: {
                name: 'German',
                flag: 'üá©üá™',
                flashcards: [
                    { word: 'Hallo', translation: 'Hello', example: 'Hallo, wie geht\'s?' },
